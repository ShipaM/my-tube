{
	"name": "MyTube-api",
	"version": "1.0.0",
	"description": "Video streaming platform backend API",
	"author": "MyTube",
	"license": "MIT",
	"scripts": {
		"build": "nest build",
		"start": "nest start",
		"dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"format": "prettier --write \"src/**/*.ts\"",
		"db:seed": "prisma generate && prisma db push && tsx prisma/seed.ts",
		"db:reset": "prisma db push --force-reset --accept-data-loss && tsx prisma/seed.ts",
		"db:studio": "prisma studio --port=7777"
	},
	"dependencies": {
		"@faker-js/faker": "^9.3.0",
		"@nestjs/common": "^11.1.9",
		"@nestjs/config": "^4.0.2",
		"@nestjs/core": "^11.1.9",
		"@nestjs/jwt": "^11.0.1",
		"@nestjs/mapped-types": "^2.1.0",
		"@nestjs/platform-express": "^11.1.9",
		"@nestjs/serve-static": "^5.0.4",
		"@nestlab/google-recaptcha": "^3.10.0",
		"@prisma/client": "^6.9.0",
		"argon2": "^0.44.0",
		"axios": "^1.13.2",
		"cookie-parser": "^1.4.7",
		"ffmpeg-static": "^5.3.0",
		"ffprobe-static": "^3.1.0",
		"form-data": "^4.0.5",
		"helmet": "^8.1.0",
		"mailgun.js": "^12.4.0",
		"nestjs-zod": "^5.0.1",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.2",
		"zod": "^3.25.56"
	},
	"devDependencies": {
		"@nestjs/cli": "^11.0.7",
		"@nestjs/schematics": "^11.0.5",
		"@types/cookie-parser": "^1.4.8",
		"@types/express": "^5.0.3",
		"@types/multer": "^1.4.12",
		"@types/node": "^22.15.29",
		"eslint": "^9.28.0",
		"prettier": "^3.5.3",
		"prisma": "^6.9.0",
		"tsconfig-paths": "^4.2.0",
		"tsx": "^4.19.4",
		"typescript": "^5.8.3"
	}
}
